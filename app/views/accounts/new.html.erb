<h1>Accounts#new</h1>

<%= form_with(model: @account, local: true) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, require: true, placeholder: 'Account Type' %>

  <%= f.label :balance %>
  <%= f.number_field :balance, require: true, placeholder: 'Balance Amount' %>

  <%= f.submit %>

<% end %>
